[global_config]
  borderless = True
  extra_styling = False
  title_hide_sizetext = True
  title_use_system_font = False
  title_font = FuraCode Nerd Font Mono 10
[keybindings]
[profiles]
  [[default]]
    background_color = "#282a36"
    cursor_color = "#aaaaaa"
    font = FuraCode Nerd Font Mono Medium 10
    foreground_color = "#44475a"
    palette = "#000000:#aa0000:#00aa00:#aa5500:#0000aa:#aa00aa:#00aaaa:#aaaaaa:#555555:#ff5555:#55ff55:#ffff55:#5555ff:#ff55ff:#55ffff:#ffffff"
    use_system_font = False
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
[plugins]
